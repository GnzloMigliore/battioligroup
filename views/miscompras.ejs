<!DOCTYPE html>

<html lang="en">
<!-- head -->
<%- include('partials/head.ejs') %>
<!-- end head -->
<!-- body start-->

<body>

<!-- end preloader -->
     <!-- top bar start-->
	 <div id="top-bar" class="top-bar-main-block">
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					
				  
				</div>
				<div class="col-md-6 text-right">
					<div class="top-bar-social">
						<ul>
							<li class="call"><i class="las la-phone-volume"></i>Llamanos: <a href="tel:" title="">+1 (305) 775 - 9044</a></li>
							<li><a href="https://www.instagram.com/battioligroup/?hl=es-la" target="_blank" title="google-plus"><ion-icon name="logo-instagram"></ion-icon></a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- top bar end-->
	<!-- top-nav bar start-->
	<div id="nav-bar" class="nav-bar-main-block">
		<div class="sticky-area">
			<div class="container">
				<div class="row">
					<div class="col-lg-3 col-md-12">
						<!--logo-->
						<div class="logo">
							<a href="/" title="logo"><img src="/images/logobattioli.png" alt="logo"></a>
						</div>
						<!-- Responsive Menu Area -->
						<div class="responsive-menu-wrap"></div>
					</div>
	
					<div class="col-lg-7">
						<!-- main-menu-->
						<div class="navigation text-white">
							<div id="cssmenu">
								<ul>
								
									<li class="active"><a href="/" title="Home">Home</a>
								
									<li><a href="/nosotros" title="Pages">Nosotros</a>
									
									</li>
									<li><a href="/servicios" title="services">Servicios</a>
										
									</li>
								
								
									<li><a href="/contacto" title="contact">Contact</a>
                                    </li>
                                   
                                    <li>
                                         <% usuario.forEach(usuario => { %><a href="/miscompras/<%= usuario.id%>" title="contact">Mis compras</a>
                                         <% }) %>
                                    </li>
                                 
								</ul>
							</div>
						</div>
					</div>
					<div class="col-lg-2 col-md-2">
                        <div class="dropdown mt-3">
                            <% usuario.forEach(usuario => { %>
                                <button class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                                    <%= usuario.first_name + " " + usuario.last_name%> 
                                </button>
                                <% }) %>
                           
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                              <li><a class="dropdown-item" href="#">Perfil</a></li>
                              <li><a class="dropdown-item" href="/logout">Cerrar sesión</a></li>
                              
                            </ul>
                          </div>
					</div>
					
				</div>
			</div>
		</div>
	</div>

	<div class="breadcroumb-area bread-bg">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="breadcroumb-title text-center">
						<h1>Mis compras</h1>
						<h6><a href="index.html">Home</a> /Administar</h6>
					</div>
				</div>
			</div>
		</div>
	</div>
   <article>
	   
    <% if (producto.length == 0) {%>
        <h4 class="w-100 text-center my-5">NO HAS REALIZADO NINGUNA COMPRA</h4>
    <%}else{%>
        <table class="table">
            <thead>
                <tr>
                    
                    <th scope="col">Nombre del producto</th>
                    <th scope="col">Estado del envío</th>
					<th scope="col">Peso</th>
					<th scope="col">Número de tracking</th>
                    <th scope="col">Fecha</th>
              
                                       
                </tr>
            </thead>
            <tbody>
                <% producto.forEach(product=> { %>
                    <tr>
                        <td><%= product.name_product %></td>
                        <td><%= product.place %></td>
						<td><%= product.weight %></td>
						<td><%= product.n_tracking %></td>
                         <td><%= product.date_state %></td>
                      
                       
                    </tr>
                    <% }); %> 
            </tbody>
        </table>
    <% } %>
    <a href="/" class=" btn btn-primary border mb-3 ml-5">Volver</a>
</article>
	

	
<!-- footer start-->
<%- include('partials/footer.ejs') %>
<!-- footer end-->
	

	<!-- Jquery Library -->
	<script src="js/jquery-2.min.js"></script>
	<!-- Bootstrap Js -->
	<script src="js/bootstrap.bundle.js"></script>
	<!-- Owl Carousel js -->
	<script src="js/owl.carousel.min.js"></script>
	<!-- Smooth Scroll Js -->
	<script src="js/smooth-scroll.js"></script>
	<!-- Navigation JS -->
	<script src="js/menumaker.js"></script>
	<!-- Way Points JS -->
	<script src="js/waypoints.min.js"></script>
	<!-- Counter Up JS -->
	<script src="js/jquery.counterup.js"></script>
	<!-- Sticky JS -->
	<script src="js/jquery.sticky.js"></script>
	<!-- Slicknav JS -->
	<script src="js/jquery.slicknav.min.js"></script>
	<!-- Mail Chimp JS -->
	<script src="vendor/mailchimp/jquery.ajaxchimp.js"></script>
	<!-- Main JS -->
	<script src="js/theme.js"></script>
	<!-- end JS -->
	<!-- Ionicons -->
	<script  src = "https://unpkg.com/ionicons@5.1.2/dist/ionicons.js" ></script>
</body>
<!-- body end -->

</html>
